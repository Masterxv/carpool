<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="@dimen/c_20dp"
    android:background="@drawable/background_route_leader_item">

    <!--<android.support.constraint.ConstraintLayout-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="match_parent"-->
    <!--android:background="@drawable/recommend_button">-->

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/c_15dp"
        android:layout_marginTop="@dimen/c_15dp"
        android:layout_marginEnd="@dimen/c_15dp"
        android:layout_marginBottom="@dimen/c_7dp"
        android:background="@drawable/recommend_button_my"
        app:layout_constraintBottom_toTopOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:textStyle="bold"
        android:id="@+id/item_route_leader_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="06月03日 08:30 周一"
        android:textColor="#000000"
        android:textSize="@dimen/c_17dp"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.06" />

    <ImageView
        android:id="@+id/item_route_leader_delete"
        android:layout_width="@dimen/c_16dp"
        android:layout_height="@dimen/c_16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="@dimen/c_24dp"
        android:src="@drawable/route_delete"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <View
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="0.5dp"
        android:layout_marginStart="@dimen/c_20dp"
        android:layout_marginEnd="@dimen/c_20dp"
        android:background="#ECECEC"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.19" />


    <TextView
        android:id="@+id/textView5"
        android:layout_width="@dimen/c_20dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:gravity="bottom"
        android:text="起"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.28" />

    <TextView
        android:id="@+id/item_route_leader_firstPlace"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/c_24dp"
        android:layout_marginEnd="@dimen/c_8dp"
        android:ellipsize="end"
        android:singleLine="true"
        android:text="重庆邮电大学"
        android:textColor="#000000"
        android:textSize="@dimen/c_17dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="@dimen/c_20dp"
        android:layout_height="wrap_content"
        android:gravity="bottom"
        android:text="终"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.4" />

    <TextView
        android:textStyle="bold"
        android:id="@+id/item_route_leader_endPlace"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/c_8dp"
        android:ellipsize="end"
        android:singleLine="true"
        android:text="重庆工商大学 "
        android:textColor="#000000"
        android:textSize="@dimen/c_17dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/item_route_leader_firstPlace"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/c_10dp"
        android:layout_marginBottom="8dp"
        android:gravity="bottom"
        android:text="队长"
        app:layout_constraintBottom_toTopOf="@+id/view5"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.57" />

    <View
        android:id="@+id/view2"
        android:layout_width="@dimen/c_5dp"
        android:layout_height="@dimen/c_5dp"
        android:layout_marginStart="@dimen/c_6dp"
        android:background="@drawable/route_oval_gray"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="@+id/item_route_leader_endPlace"
        app:layout_constraintTop_toTopOf="@+id/textView7" />

    <TextView
        android:id="@+id/item_route_leader_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/c_8dp"
        android:layout_marginEnd="8dp"
        android:text="小明"
        android:textColor="#000000"
        android:textSize="@dimen/c_17dp"
        android:singleLine="true"
        android:ellipsize="end"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="bottom"
        android:text="队友"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:layout_constraintVertical_bias="0.67" />


    <View
        android:visibility="gone"
        android:id="@+id/view7"
        android:layout_width="@dimen/c_5dp"
        android:layout_height="@dimen/c_5dp"
        android:background="@drawable/route_oval_purple"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/textView8" />

    <TextView
        android:id="@+id/item_route_leader_team_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="匹配拼友中..."
        android:singleLine="true"
        android:ellipsize="end"
        android:textColor="#f4b712"
        android:textSize="@dimen/c_17dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="@+id/item_route_leader_name"
        app:layout_constraintStart_toStartOf="@+id/item_route_leader_name"
        app:layout_constraintTop_toTopOf="@+id/textView8" />

    <View
        android:id="@+id/view4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="@dimen/c_15dp"
        android:layout_marginTop="@dimen/c_8dp"
        android:background="@drawable/recommend_button_my"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/view5" />

    <View
        android:id="@+id/view3"
        android:layout_width="0dp"
        android:layout_height="@dimen/c_1dp"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="@dimen/c_20dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="@dimen/c_20dp"
        android:layout_marginBottom="8dp"
        android:background="#ECECEC"
        app:layout_constraintBottom_toTopOf="@+id/view5"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/view"
        app:layout_constraintTop_toBottomOf="@+id/item_route_leader_team_name"
        app:layout_constraintVertical_bias="0.15" />


    <TextView
        android:id="@+id/route_leader_over"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/c_6dp"
        android:gravity="center"
        android:text="行程完结"
        android:textColor="#969696"
        android:textSize="@dimen/c_16dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toStartOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="@dimen/c_1dp"
        android:layout_height="@dimen/c_13dp"
        android:background="#AFAFAF"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/view3"
        app:layout_constraintVertical_bias="0.45" />

    <TextView
        android:id="@+id/route_leader_com"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/c_8dp"
        android:gravity="center"
        android:text="立刻交流"
        android:textColor="#969696"
        android:textSize="@dimen/c_16dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView9" />


    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="申请名单"
        android:textColor="#000000"
        android:textSize="@dimen/c_17dp"
        app:layout_constraintBottom_toBottomOf="@+id/view4"
        app:layout_constraintStart_toStartOf="@+id/item_route_leader_time"
        app:layout_constraintTop_toTopOf="@+id/view4"
        app:layout_constraintVertical_bias="0.06" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="0dp"
        android:layout_height="0.6dp"
        android:background="#ECECEC"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/view3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:layout_constraintVertical_bias="0.041" />

    <TextView
        android:id="@+id/no_apply_people_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="别 急 哦，拼 友 马 上 来 ~"
        app:layout_constraintBottom_toBottomOf="@+id/view4"
        app:layout_constraintEnd_toEndOf="@+id/view4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/view4"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        app:layout_constraintVertical_bias="0.371" />

    <com.campussay.carpool.ui.route.ChildRecyclerView
        android:id="@+id/route_recycler_apply_now"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/c_10dp"
        android:layout_marginBottom="@dimen/c_8dp"
        android:scrollbars="none"
        app:layout_constraintBottom_toBottomOf="@+id/view4"
        app:layout_constraintEnd_toEndOf="@+id/textView13"
        app:layout_constraintStart_toStartOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/textView13">

    </com.campussay.carpool.ui.route.ChildRecyclerView>

    <!--<View-->
    <!--android:layout_width="0dp"-->
    <!--android:layout_height="@dimen/c_60dp"-->
    <!--app:layout_constraintBottom_toBottomOf="@+id/view4"-->
    <!--app:layout_constraintEnd_toEndOf="@+id/view4"-->
    <!--android:background="@drawable/background__route_leader_bottom"-->
    <!--app:layout_constraintStart_toEndOf="@+id/no_apply_people_tv" />-->

</android.support.constraint.ConstraintLayout>

